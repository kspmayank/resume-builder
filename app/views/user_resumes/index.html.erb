<p id="notice"><%= notice %></p>

<h1>User Resumes</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Template</th>
      <th>Content</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_resumes.each do |user_resume| %>
      <tr>
        <td><%= user_resume.user %></td>
        <td><%= user_resume.template %></td>
        <td><%= user_resume.content %></td>
        <td><%= link_to 'Show', user_resume %></td>
        <td><%= link_to 'Edit', edit_user_resume_path(user_resume) %></td>
        <td><%= link_to 'Destroy', user_resume, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User Resume', new_user_resume_path %>
